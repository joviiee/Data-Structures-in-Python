   "source": [
    "class Gcd():\n",
    "    def euclidean_algo(self,x,y):\n",
    "        if x < y:\n",
    "            x, y = y, x\n",
    "        while x%y != 0:\n",
    "            remainder = x%y\n",
    "            x, y = y, remainder\n",
    "        return y"
   ]
  },
